public class methodsUse 
{    
    public static void main(String[] args)     
    {    
        double p = 78;    
        double q = 5;    
          
        // return the maximum of two numbers  
        System.out.println("Maximum number of p and q is: " +Math.max(p, q));   
          
        // return the square root of y   
        System.out.println("Square root of q is: " + Math.sqrt(q));   
          
        //returns 28 power of 4 i.e. 28*28*28*28    
        System.out.println("Power of p and q is: " + Math.pow(p, q));      
  
        // return the logarithm of given value       
        System.out.println("Logarithm of p is: " + Math.log(p));   
        System.out.println("Logarithm of q is: " + Math.log(q));  
          
        // return the logarithm of given value when base is 10      
        System.out.println("log10 of p is: " + Math.log10(p));   
        System.out.println("log10 of q is: " + Math.log10(q));    
          
        // return the log of p + 1  
        System.out.println("log1p of x is: " +Math.log1p(p));    
  
        // return a power of 2    
        System.out.println("exp of a is: " +Math.exp(p));    
          
        // return (a power of 2)-1  
        System.out.println("expm1 of a is: " +Math.expm1(p));  
    }    
}    
