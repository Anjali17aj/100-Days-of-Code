2-D array
IDE used : GFG


import java.util.*;
import java.io.*;
import java.lang.*;



class twodarrays
{ 

	public static void main(String args[]) 
    {
    	int arr[][] = {{1, 2, 3, 7, 10},
    				   {4, 5, 6,11 ,17}};

    	for(int i = 0; i < arr.length; i++)
    	{
    		for(int j = 0; j < arr[i].length; j++)
    		{
    			System.out.print(arr[i][j] + " ");
    		}
    	}
    } 

}

